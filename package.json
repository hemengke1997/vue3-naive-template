{
  "name": "lagofast-vue3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build --mode production",
    "build:test": "cross-env NODE_ENV=test vite build --mode test",
    "preview": "vite preview",
    "serve": "npm run build:test && npm run preview",
    "report": "cross-env REPORT=true npm run build:test",
    "release": "esno scripts/release.ts",
    "release:tag": "esno scripts/release.ts --tag",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "upgrade:deps": "pnpm update --i --L",
    "pnpm:prune": "pnpm prune",
    "debug": "vite --debug",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf node_modules && pnpm i"
  },
  "dependencies": {
    "@vueuse/core": "^8.5.0",
    "@vueuse/shared": "^8.5.0",
    "axios": "^0.27.2",
    "colord": "^2.9.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.2",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.29.0",
    "pinia": "^2.0.14",
    "query-string": "^7.1.1",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.35",
    "@types/prompts": "^2.0.14",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.36",
    "autoprefixer": "^10.4.7",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "magic-string": "^0.26.2",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "semver": "^7.3.7",
    "stylelint": "^14.8.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imp": "^2.1.8",
    "vite-plugin-svgr": "^2.1.0",
    "vite-plugin-windicss": "^1.8.4",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7",
    "windicss": "^3.5.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup"
      ]
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --ext",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,css,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,.json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  }
}
